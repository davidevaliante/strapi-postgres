{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "name": "Tag"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "posts": {
      "via": "tags",
      "collection": "post"
    },
    "post": {
      "via": "mainTag",
      "model": "post"
    }
  }
}
